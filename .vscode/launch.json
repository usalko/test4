    "version": "0.2.0",
    "configurations": [
        {
            "name": "Service: Development",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/service/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "justMyCode": false
        },
        {
            "name": "Front: Development",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
    ]
}